; version 4
; [X -> Y] Y List-of-X -> Y
(define (f*ldl f i l0)
  (local ((define (foldr/a a l)
            (cond
              [(empty? l) a]
              [else (foldr/a (f (first l) a) (rest l))])))
    (foldr/a i l0)))
