(build-list 10 add1)
(build-list 10 sub1)

(define (add-at-end l e)
  (cond
    [(empty? l) (cons e '())]
    [else
     (cons (first l) (add-at-end (rest l) e))]))

; X [X Y -> Y] Y -> Y
; Number [Number -> X] -> [List-of X]
(define (build-l*st n f) 
  (cond [(zero? n) '()]
        [else (add-at-end (build-l*st (sub1 n) f) (f (sub1 n))) ])
  )


(build-l*st 10 sub1)
(build-l*st 10 add1)