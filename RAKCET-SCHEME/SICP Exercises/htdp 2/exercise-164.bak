(define empty-lot '())
(define sample-lot (cons 100 (cons 35 '())) )


(define (f-to-c n)
  (* (- n 32) (/ 5 9))
  )



(define (convertFC lot)
  (cond [(empty? lot) '()]
        [else (cons (f-to-c (first lot)) (convertFC (rest lot)) ) ]
        )
  )

