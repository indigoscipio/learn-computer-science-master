; List-of-string String -> N
; determines how often s occurs in los

;List-of-string is either
; '()
; cons String List-of-string

(define (count los s)
  ;base case: check if the list if empty; if its empty return 0
  (cond [(empty? los) 0]
        [(equal? s (first los)) (add1 (count (rest los) s))]
        [else (count (rest los) s) ]
        )
  )

(define sample-los (cons "apple " (cons "banana" (cons "apple" '())))  )
(count sample-los "apple")