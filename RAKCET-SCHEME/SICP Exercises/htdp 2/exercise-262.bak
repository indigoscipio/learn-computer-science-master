(define-struct IR
  [name price])
; An IR is a structure:
;   (make-IR String Number)
; An Inventory is one of: 
; – '()
; – (cons IR Inventory)
(define sample-ir1 (make-IR "Name1" 0))
(define sample-ir2 (make-IR "Name2" 11))
(define sample-ir3 (make-IR "Name3" 23))
(define sample-ir-list (list sample-ir1 sample-ir2 sample-ir3))



; Inventory -> Inventory 
; creates an Inventory from an-inv for all
; those items that cost less than a dollar
(define (extract1 an-inv)
  (cond
    [(empty? an-inv) '()]
    [else (local ((define recurse-an-inv (extract1 (rest an-inv))))
            (if (<= (IR-price (first an-inv)) 1.0)
                (cons (first an-inv) recurse-an-inv)
                recurse-an-inv
                )
            )
 
          ]))
(extract1 sample-ir-list)



#|
    (cond
       [(<= (IR-price (first an-inv)) 1.0)
        (cons (first an-inv) (extract1 (rest an-inv)))]
       [else (extract1 (rest an-inv))])

