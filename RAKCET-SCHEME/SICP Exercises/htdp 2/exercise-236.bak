;Use the contains? function to define functions
;that search for "atom", "basic", and "zoo", respectively.
(define sample-list (list "dog" "cat"))

(define (contains? s l)
  (cond [(empty? l) #false]
        [else (or (equal? s (first l)) (contains? s (rest l))) ])
  )


; Los -> Boolean
; does l contain "dog"
(define (contains-dog? l)
  (contains? "dog" l))

(define (contains-atom? l)
  (contains? "atom" l)
  )


(define (contains-basic? l)
  (contains? "basic" l)
  )


(define (contains-zoo? l)
  (contains? "zoo" l)
  )

