;[X -> Y] List-of-X -> List-of-Y
(define (my-map f l)
  (local (; [X -> Y] List-of-X List-of-Y -> List-of-Y
          ; acc accumulates the result of applying f to each element of l0,
          ; but in reverse order
          (define (my-map/a f l0 acc)
            (cond [(empty? l0) (reverse acc)] ;base case: if its empty all of the function is applied, return acc
                  [else (my-map/a f (rest l0) (cons (f (first l0)) acc) ) ]
                  )
            ))
    (my-map/a f l '())
    )
  )
(my-map add1 '(1 2 3 4 5))